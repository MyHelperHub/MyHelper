// @ts-nocheck

<template>
  <div>
  <button @click="getConfig" > Get Config < /button>
    < button @click="setConfig" > Set Config < /button>
      < button @click="deleteConfig" > Delete Config < /button>
        < /div>
        < /template>

        < script setup >
import { invoke } from '@tauri-apps/api';

// 调用 get_config 命令
const getConfig = async () => {
  try {
    const keys = ['position'];
    const result = await invoke('get_config', { keys });
    console.log('Config:', result);
  } catch (error) {
    console.error('Error getting config:', error);
  }
};

// 调用 set_config 命令
const setConfig = async () => {
  try {
    const data = {
      position: {
        x: 100,
        y: 200
      }
    };
    await invoke('set_config', { data });
    console.log('Config set successfully');
  } catch (error) {
    console.error('Error setting config:', error);
  }
};

// 调用 delete_config 命令
const deleteConfig = async () => {
  try {
    const key = 'position';
    await invoke('delete_config', { key });
    console.log('Config deleted successfully');
  } catch (error) {
    console.error('Error deleting config:', error);
  }
};
</script>
